    "def pot4(n):\n",
    "    if n < 1:\n",
    "        return False\n",
    "    if n == 1:\n",
    "        return True\n",
    "    if n % 4 == 0:\n",
    "        return pot4(n/4)\n",
    "    else:\n",
    "        return False"
   ]
